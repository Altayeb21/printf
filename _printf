                        count++;

int _printf(const char *format, ...) {
    int count = 0;
    va_list args;
    va_start(args, format);
    for (const char *p = format; *
    ; ++p) {
        if (*p != '%') {
            putchar(*p);
            count++;
            
        } else {
            p++;
            switch (*p) {
                case 'c': {
                    char c = (char va_arg(args, int);
                    putchar(c);
                    count++;
                    break;
                    
                }
                case 's': {
                    const char *s = va_arg(args, const char *);
                    for (const char*q = s; *q; ++q) {
                        putchar(*q);
                        count++;
                        
                    }
                    break;
                    
                }
                case '%': {
                    putchar('%');
                    count++;
                    break;
                    
                }
                default: {
// If the format string contains any other character after the %, it will be treated as a normal character.
putchar('%');
putchar(*p);
count += 2;
break;
                }
            }
       }
    }
    va_end(args);
    return count;
}

int main() {
    _printf("Hello, World! %c\n", 'A');
    _printf("Sum: %d\n", 5 + 10); _printf("Pi: %.2f\n", 3.14159265358979323846); _printf("100%% Apple Juice!\n");
    return 0;
}
