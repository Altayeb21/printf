int _printf(const char *format, ...) {
    int count = 0;
    va_list args;
    va_start(args, format);
    
    for (const char *p = format; *p; ++p) {
        if (*p != '%') {
            putchar(*p);
            count++;
            
        } else {
            p++;
            switch (*p) {
